			$("#follow").val(event);
			follow = 'top'
		}

		ajax_data(strlimit, endlimit);
	}


function ajax_data0(strlimit, endlimit,follow) {
	
	var search_txt = $("input[name='search_text']").val();
	//var orgmem_seqNo = $("#orgSeqList").val();
	var search_seqNo = $("#search_seqNo").val();
	var search_major = $("#search_major").val();
	var search_area = $("#search_area").val();
	
	$.ajax({
        type:"POST",
        url:"/member_list_data",
        datatype:"json",
		data:{strlimit : strlimit, endlimit : endlimit, search_text : search_txt, search_seqNo : search_seqNo, search_major : search_major, search_area : search_area,follow:follow,m_check:m_check },
        success : function(json) {
			$("#mbr_limitAra").empty();
			var ment = makeText(json.db_data, json.result);
       		$("#mbr_limitAra").append(ment);
       		$("#member_count").text(json.db_data_length);
       		//$("#member_count").text(json.db_data_length + "(" + json.app_install_count +")"); 앱설치수 숨김
       		$("input[name='jsonlength']").val(json.db_data_length); 
        },
        beforeSend : function(){
        	$('.wrap-loading').removeClass('display-none');
        },
        complete : function(){
        	$('.wrap-loading').addClass('display-none');
        },
        error : function(xhr, status, error) {
        	alert("등록된 회원이 없습니다.");
        } 
  	});
}
	
