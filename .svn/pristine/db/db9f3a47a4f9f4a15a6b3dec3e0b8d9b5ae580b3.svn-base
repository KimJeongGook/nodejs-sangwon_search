<script type="text/javascript" src="/js/jquery/jquery-1.7.1.min.js"></script>

<script type="text/javascript">
$(document).ready(function() {
	$('#print_start').click(function(){
		$("#print_start").hide();
		$("#back_url").hide();
		$("#h_style").css("height", "0px");
		
		window.print();
		
		$("#print_start").show();
		$("#back_url").show();
		$("#h_style").css("height", "40px");
	});
	
	$('#back_url').click(function(){
		var popup = window.open("", "" , "width=1,height=1");
		popup.close();
		
		var form = document.writehtm;
		var url = '/search_data';
		
		form.action = url;
		form.target = '_self';
		form.submit();
	});
	
});
</script>

<!-- 프린트 할 때 -->
<style type="text/css" media="print">
	@page { size: auto; margin-top: 52px; margin-right: 18px; margin-left: 18px;}
</style>

<!-- PC화면으로 볼때 -->
<style>
	/* 폰트 굵은거 */
	@font-face {
	    font-family: 'NEXON Lv1 Gothic OTF Bold';
	    src: url('https://cdn.jsdelivr.net/gh/projectnoonnu/noonfonts_20-04@2.1/NEXON Lv1 Gothic OTF Bold.woff') format('woff');
	    font-weight: normal;
	    font-style: normal;
	}
	
	/* 폰트 얇은거 */
	@font-face {
	    font-family: 'NEXON Lv2 Gothic';
	    src: url('https://cdn.jsdelivr.net/gh/projectnoonnu/noonfonts_20-04@2.1/NEXON Lv2 Gothic.woff') format('woff');
	    font-weight: normal;
	    font-style: normal;
	}
	
	.n_tit, .a_tit, .z_tit {font-family: 'NEXON Lv1 Gothic OTF Bold';}
	.index_box .addr, .index_box .company {font-family: 'NEXON Lv2 Gothic';}
	
	body {margin: 0; padding: 0;}
	div {color: #000000; /* font-family: '굴림',Gulim; */ font-size: 14px;}
	.btn_wrap {text-align: right;}
	.index_box {text-align: center; float: left; width: 370px; box-sizing: border-box; color: #000000; overflow: hidden; height: 127px; padding: 10px;} 
	.index_box .addr {text-align: left; margin-bottom: 3px; height: 33px;}
	.index_box .company {text-align: left; margin-left: 20px; height: 35px;}
	.index_box .op_box {margin-left: 80px; margin-top: 8px; font-weight: bold;}
	.index_box .op_box .name{font-size: 17px; margin-top: -10px; font-weight: bold;}
	.index_box .op_box .addr_code{font-size: 17px; margin-top: -5px;}
	.index_box .op_box .name .n_tit {margin-right: 0px;}
	
	@media print {
		/* 폰트 굵은거 */
		@font-face {
		    font-family: 'NEXON Lv1 Gothic OTF Bold';
		    src: url('https://cdn.jsdelivr.net/gh/projectnoonnu/noonfonts_20-04@2.1/NEXON Lv1 Gothic OTF Bold.woff') format('woff');
		    font-weight: normal;
		    font-style: normal;
		}
		
		/* 폰트 얇은거 */
		@font-face {
		    font-family: 'NEXON Lv2 Gothic';
		    src: url('https://cdn.jsdelivr.net/gh/projectnoonnu/noonfonts_20-04@2.1/NEXON Lv2 Gothic.woff') format('woff');
		    font-weight: normal;
		    font-style: normal;
		}
		
		#header{display: none;}
		#footer{display: none;}
		
		body * {visibility: visible; -webkit-print-color-adjust: exact !important; background: #fff;}
		
		.n_tit {visibility: visible; font-size: 17px; font-weight: bold;}
		
		.a_tit {visibility: visible; font-size: 14px; margin-left: 8px;}
		
		.z_tit {visibility: visible; font-size: 17px;}
		
		.n_tit, .a_tit, .z_tit {font-family: 'NEXON Lv1 Gothic OTF Bold';}
		.index_box .addr, .index_box .company {font-family: 'NEXON Lv2 Gothic';}
	}
</style>

