				$(".dispatch_pop_close").click();
			}
}
	function search_areah(event){
		console.log(event);
		strlimit = 0;
		endlimit = 20;
		num = 0;
		var search_txt = $("input[name='search_text']").val();
		//var orgmem_seqNo = $("#orgSeqList").val();
		var orgmem_seqNum = $("#orgmem_seqNum").val();
		$("#mbr_limitAra").html("");
		var follow = '';
		if(event == 'follow'){
			$("#follow").val(event);
			follow = 'start'
		}else if(event == 'money'){
			$("#follow").val(event);
			follow = 'money'
