		<script type="text/javascript">
			$(document).ready(function(){
				setTimeout(function(){
					$('.top_title').prepend(sessionStorage.getItem('userName'));
				},10)
				var menu = "<%=menuNum%>";
				var subMenuNum = "<%=subMenuNum%>";
				
				$('#lnb ul li').eq(menu).addClass('on');
				
				if($('#Gong').hasClass('on')){
					$('#TopSubMenuG').show();
				}else{
					$('#TopSubMenuG').hide();
				}
				
				if($('#Gall').hasClass('on')){
					$('#TopSubMenuGa').show();
				}else{
					$('#TopSubMenuGa').hide();
				}
				
				if($('#enjoy').hasClass('on')){
					$('#TopSubMenuEnjoy').show();
					if(subMenuNum == 0){
						$('#select_r').addClass('on');
					} else if(subMenuNum == 1){
						$('#select_a').addClass('on');
					} else if(subMenuNum == 2){
						$('#select_b').addClass('on');
					} else if(subMenuNum == 3){
						$('#select_c').addClass('on');
					} else if(subMenuNum == 4){
						$('#select_n').addClass('on');
					}
				}else{
					$('#TopSubMenuEnjoy').hide();
				}
				
				if($('#complaint').hasClass('on')){
					$('#TopSubMenuComplaint').show();
					if(subMenuNum == 0){
						$('#select_0').addClass('on');
					} else if(subMenuNum == 1){
						$('#select_1').addClass('on');
					} else if(subMenuNum == 2){
						$('#select_2').addClass('on');
					} else if(subMenuNum == 3){
						$('#select_3').addClass('on');
					} else if(subMenuNum == 4){
						$('#select_4').addClass('on');
					}
				}else{
					$('#TopSubMenuComplaint').hide();
				}
				
				if($('#sms').hasClass('on')){
					$('#TopSubMenuSMS').show();
					if(subMenuNum == 0){
						$('#select_00').addClass('on');
					} else if(subMenuNum == 1){
						$('#select_01').addClass('on');
					} else if(subMenuNum == 2){
						$('#select_02').addClass('on');
					} else if(subMenuNum == 3){
						$('#select_03').addClass('on');
					} else if(subMenuNum == 4){
						$('#select_04').addClass('on');
					} else if(subMenuNum == 5){
						$('#select_05').addClass('on');
					} else if(subMenuNum == 6){
						$('#select_06').addClass('on');
					}
				}else{
					$('#TopSubMenuSMS').hide();
				}
				
			});
			
			function MM_openBrWindow_company(theURL,winName,features,number,mode) {
				theURL += '?mode=' + mode + '&number=' + number;
				window.open(theURL,winName,features);
			}
			
			function MM_openBrWindow_view_company(theURL,winName,features,number,mode,industry1, industry2, industry3) {
				theURL += '?mode=' + mode + '&number=' + number + '&industry1=' + industry1 + '&industry2=' + industry2 + '&industry3=' + industry3;
				window.open(theURL,winName,features);
			}
			
			function MM_openBrWindow(theURL,winName,features,number,mode,category) {
				if(number != '')
					theURL += '?mode=' + mode + '&number=' + number + '&category=' + category;
				else
					theURL += '?mode=' + mode + '&category=' + category;
				window.open(theURL,winName,features);
			}
			
			function MM_openBrWindow_sms(theURL,winName,features,number,mode) {
				theURL += '?mode=' + mode + '&number=' + number;
				window.open(theURL,winName,features);
			}
			
			function MP_openBrWindow(theURL,winName,features) {
				window.open(theURL,winName,features);
			}
			
			function AN_openBrWindow(theURL,winName,features,number) {
				theURL += '?orgmem_no=' + number;
				window.open(theURL,winName,features);
			}
			
			function MT_openBrWindow(theURL,winName,features,number) {
				theURL += '?orgmem_no=' + number;
				window.open(theURL,winName,features);
			}
			
			function winOpen(){
				var win = window.open('/member_list', 'member_list','width=540,height=920,scrollbars=yes');
				win.focus();
			}
			
	
		</script>
