@charset "utf-8";
@import url(//fonts.googleapis.com/earlyaccess/notosanskr.css);
@import url('https://fonts.googleapis.com/css2?family=Black+Han+Sans&display=swap');
*{font-family: 'Noto Sans KR';}

html {height:100%;}
body {margin:0; padding:0; height:100%; background:url(/images/search_background3.png); background-repeat: no-repeat; background-size: cover;}
div {margin:0; padding:0; color:#111; font-size:1em;  line-height:130%; font-size:12px;}

input, textarea, select {margin:0; padding:0;}
form {display:block; border:none;}
fieldset {margin:0px; padding:0px; border:none;}
input {padding:0px; vertical-align:middle; border:none;}
input[type~="text"] ,input[type~="password"]  {border:1px solid #ccc; padding:1px;}
input[type~="file"] {border:1px solid #eaeaea; padding:2px 0 2px 5px;}
input[type~="checkbox"] {border:none; margin:-1px 0; padding:2px 0 2px 5px;}
input[type~="radio"] {border:none; margin:-1px 0; padding:2px 0 2px 5px;}
input[type~="button"]{padding-top:3px; cursor:pointer;outline:none;}
input[type~="submit"]{padding-top:3px; cursor:pointer;}
input[type~="image"]{margin:0; padding:0; cursor:pointer;}

select {vertical-align:middle; padding:0;}
textarea {padding:2px 0 2px 5px; border:1px solid #ccc;}

a {margin:0; padding:0; -webkit-text-size-adjust:none;}

p {margin:0; padding:0; line-height:130%;}

strong {margin:0; padding:0; font-family:'돋움',dotum;}
Form {display:inline; margin:0; padding:0;}
img {border:none; margin:0; padding:0;}
fieldset {margin:0; padding:0; border:0;}
table {margin:0;  border-collapse:separate; table-layout:fixed; -webkit-border-horizontal-spacing : 20px;}
td{word-break:break-all;}
th{word-break:break-all;}

ul,li,ol,dl,dd,dt {margin:0; padding:0; list-style:none; overflow:hidden;}
hr {padding:0; margin:0;}

h1 {margin:0; padding:0; -webkit-text-size-adjust:none;}
h2 {margin:0; padding:0; -webkit-text-size-adjust:none;}
h3 {margin:0; padding:0; -webkit-text-size-adjust:none;}
h4 {margin:0; padding:0; -webkit-text-size-adjust:none;}
h5 {margin:0; padding:0; -webkit-text-size-adjust:none;}

a:link   {color: #555; text-decoration:none; border:none;}
a:visited{color: #555; text-decoration:none; border:none;}
a:hover	 {color: #555; text-decoration:none; border:none;}
a:active {color: #555; text-decoration:none; border:none;}
a        {color: #555; text-decoration:none; border:none;}
a:focus	 {border:none;}



/*search*/
.search_title{font-weight: 400;font-size: 18px; background-color: #1e2545; color: #fff; padding: 6px 17px;text-align: center;}
.clear{clear:both; width:100%;}
.float_l{font-size: 30px;font-weight: 600;letter-spacing: 1px;margin: 3px 0;color: #232f64;}
.float_r{float:left;padding-left: 10px;}

.search_table table{width: 100%;}


.align_c    {text-align:center !important;}
.align_r    {text-align:right !important;}
.align_l    {text-align:left !important;}

.valign_t{vertical-align:top;}
.valign_m{vertical-align:middle;}
.valign_b{vertical-align:bottom;}

.strong{font-weight:bold;}
.f_normal{ font-weight:normal !important;}
.nobg{background:none !important;}
.noline{border:none !important;}

.nomargin{margin:0 !important;}
.nopadding{padding:0 !important;}

.hand{cursor:pointer;vertical-align: bottom;}
.hide{display: none;}
