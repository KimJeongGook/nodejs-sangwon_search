		<script type="text/javascript">
			$(document).ready(function(){
				var menu = "<%=menuNum%>";
				$('#lnb ul li').eq(menu).addClass('on');
				
				$.ajax({
					type : "post",  
				    url : "/admin/web/getAdminName",    
				    dataType : "text",      
				    success : function(data){
				    	$( '#adminNameField' ).text(data);
				    }, error : function(xhr, status, error) { // error 발생했을때
				    	$( '#adminNameField' ).text("error");
			      	}
				});
				
				//허용된 탑 메뉴만 보이기
				$.ajax({
					type : "post",  
				    url : "/admin/web/getChecked_power",    
				    dataType : "text",      
				    success : function(data){
				    	var chkAry = new Array();
				    	
				    	chkAry = data.split('@');
				    	
				    	$('.tmenu').children().each(function(){
				    		for(var i=0;i<chkAry.length;i++){
				    			if($(this).val() == chkAry[i]){
				    				$(this).show();
				    			}
				    		}
				    	});
				    }, error : function(xhr, status, error) { // error 발생했을때
				    	alert('에러');
			      	}
				});
			});
			
			function MM_openBrWindow(theURL,winName,features) {
				window.open(theURL,winName,features);
			}
			function winOpen(){
				var win = window.open('/member_list', 'member_list','width=540,height=900,scrollbars=yes');
				win.focus();
			}	
			function fnLogOut() {
				window.location.href='/admin/web/logout';
			}
			function go_indexUW(){
				window.location.href='/index_uw';
			}

		</script>
		
	<style>
		.tmenu li{display:none;}
	</style>
