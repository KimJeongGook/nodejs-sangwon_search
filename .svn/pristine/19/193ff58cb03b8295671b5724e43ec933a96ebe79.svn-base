	function file_del(no, name, type) {
		if(confirm("파일을 삭제하시겠습니까?")){
			$.ajax({
		        type:"POST",
		        url:"/admin/web/orgm_file_del",
		        data:{number : no, file_name : name, type : type},
		        success : function(data) {
		        	if(data == 'success'){
		        		alert("파일삭제이 삭제되었습니다.");
		        		/* window.location.reload(); */
		        		mem_ajax_data(); 
		        	}else{
		        		alert("파일삭제가 실패했습니다");
		        	}
		        },
		        error : function(xhr, status, error) {
		        	alert("파일삭제가 실패했습니다");
		        }
		  	});	
			
		}
	}

		
			function filePreview(file,target){

		var file = file.files;

		// console.log(file[0].name);

		if((file[0].name.indexOf('pdf')) < 0){ //회사소개자료
			alert("PDF파일만 업로드 가능합니다.");
			$("#company_intro").val("");
		}
		$(target.selector).append('<div style="border:1px solid black; width:50px; position:relative; left:100px;"> <input type="button" value="업로드" onclick="m_save()"></div>'); //원문 ('<div class="save_btn" onclick="m_save()"><i class="fa fa-upload"></i></div>');
		}

	function preview(input, target) {
		console.log(target.selector);
		if(input.files && input.files[0]){
		var fileName= input.files[0].name;
		var ext=fileName.substr(fileName.length-3, fileName.length);
		if(ext == "PEG" || ext == "peg"){
			ext = "jpeg"
		}
		var isCheck=false; 
			if(ext.toLowerCase()=='jpg' || ext.toLowerCase()=='gif' || ext.toLowerCase()=='png' || ext.toLowerCase()=='jpeg'){
			isCheck=true;  
			// var text = "";
			// text = '<input type="file" id="company_img1" name="company_img1" maxlength="50" ime-mode:active;" accept="image/*" onchange="preview(this,$(\'#c_img1\'));" />';
			
			// $('#company_img11').html(text);
			// var text1 = "";
			// text1 = '<input type="file" id="company_img2" name="company_img2" maxlength="50" ime-mode:active;" accept="image/*" onchange="preview(this,$(\'#c_img2\'));" />';
			// $('#company_img22').html(text1);	
		}
		$(target.selector).append('<div style="border:1px solid black; width:50px; position:relative; left:100px;"> <input type="button" value="업로드" onclick="m_save()"></div>'); //원문 ('<div class="save_btn" onclick="m_save()"><i class="fa fa-upload fa-2x"></i></div>');
		if(isCheck==false){
			alert("이미지파일만 업로드 가능합니다.");
			jQuery(input).val("");
			return;
		}
		var reader = new FileReader();
		reader.readAsDataURL(input.files[0]);          
		reader.onload = function(e) {
			jQuery(target).attr('src', e.target.result);
		}
		}
	}
